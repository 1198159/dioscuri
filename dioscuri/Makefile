#global targets
ALL:	main 
all:	main 

#object targets
%.o:	%.java
	$(GCJ) $(CP)  -c $? -o $@
	
#main target

main: $(OBJ)
	$(GCJ) $(MAINCLASS) $(OBJ) -o$(BIN)

#clean target    
clean:
	rm -f `find . -name "*.o"`
	rm -f $(BIN)

